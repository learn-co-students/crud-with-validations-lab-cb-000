<%= form_for @song do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>

  <%= f.label :released, "Is this song released?" %><br>
  <%= f.radio_button :released %><br>

  <%= f.label :release_year %><br>
  <%= f.number_field :release_year %><br>

  <%= f.label :artist_name %><br>
  <%= f.text_field :artist_name %><br>

  <%= f.label :genre %><br>
  <%= f.text_field :genre %><br>

  <%= f.submit %>
<% end %>
