<h1>All Songs</h1>

<table>
  <tr>
    <th>Title</th><th>Release Year</th><th>Artist</th><th>Genre</th><th></th>
  </tr>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= link_to song.title, song_path(song) %></td>
        <td><%= song.release_year %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.genre %></td>
        <td><%= link_to "Delete", song, method: :delete %>
      </tr>
    <% end %>
  </tbody>

</table>