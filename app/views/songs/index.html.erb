<h1>Listing Songs</h1>
 
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Released</th>
      <th>Release Year</th>
      <th>Artist Name</th>
      <th>Genre</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @songs.each do |s| %>
      <tr>
        <td><%= s.title %></td>
        <td><%= s.released %></td>
        <td><%= s.release_year %></td>
        <td><%= s.artist_name %></td>
        <td><%= s.genre %></td>
        <td><%= link_to "Show", s %></td>
        <td><%= link_to "Edit", edit_song_path(s) %></td>
        <td><%= link_to "Remove", s, method: :delete, data: { confirm: "Are you sure?" } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
 
<br>
 
<%= link_to "Create Song", new_song_path %>