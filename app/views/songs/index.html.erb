<h1>Songs</h1>

<table>
  <thead>
    <tr>
      <td>Title</td>
      <td>Artist</td>
      <td>Genre</td>
      <td>Release Year</td>
      <td>Delete?</td>
    </tr>
  </thead>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= link_to song.title, song %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.genre %></td>
        <td><%= song.release_year %></td>
        <td><%= link_to "Delete", song, method: :delete, data: {confirm: "Are you sure?"} %></td>
      </tr>
    <% end %>
  </tbody>
</table>
