<h1>Songs</h1>
<table>
  <thead>
  <tr>
    <th>Title</th>
    <th>Artist</th>
    <th>Year Released</th>
    <th>Genre</th>
    <th></th>
  </tr>
</thead>
  <tbody>
    <% @songs.each do |song| %>
      <tr>
        <td><%= song.title %></td>
        <td><%= song.artist_name %></td>
        <td><%= song.release_year if song.released? %></td>
        <td><%= song.genre %></td>
        <td>
          <%= link_to 'View', song %>
          <%= link_to 'Edit', edit_song_path(song) %>
          <%= link_to 'Delete', delete_song_path(song), method: :delete, data: { confirm: 'Sure?'} %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
