<h3> <%= @song.title %> </h3>
by:
<%= @song.artist_name %><br>
in genre:
<%= @song.genre %><br>
<% if @song.released == true %>
  <%= " released in the year: #{@song.release_year}" %>
<% else %>
  <%= "unreleased" %>
<% end %>

<%= button_to "edit", edit_song_path(@song), method: "get" %>
<%= link_to 'Delete', @song, method: :delete, data: {confirm: 'Really?'} %>
