<h1><%=@song.title%> by <%=@song.artist_name%><h1>

  <% if @song.released? %>
      <h2>Released in <%= @song.release_year %></h2>
  <% end %>

    <h2>Genre: <%= @song.genre %></h2><br><br>


<%= link_to 'Edit', edit_song_path(@song) %><br><br>
<%= link_to 'Delete', @song, method: :delete, data: { confirm: 'For real?' }%>
